{
  "mcpServers": {
    "cclsp": {
      "command": "cclsp",
      "env": {
        "CCLSP_CONFIG_PATH": "/Users/travisf/Documents/projects/insightmesh/dx/cclsp.json"
      },
      "metadata": {
        "description": "Provides semantic code navigation via Pyright LSP",
        "capabilities": [
          "Go to definition",
          "Find all references",
          "Type information",
          "Symbol search",
          "Workspace symbols"
        ],
        "languages": ["python"],
        "projectRoot": "/Users/travisf/Documents/projects/insightmesh"
      }
    },
    "claude-context": {
      "command": "npx",
      "args": ["-y", "@zilliz/claude-context-mcp"],
      "env": {
        "MILVUS_URI": "http://localhost:19530",
        "OPENAI_API_KEY": "${OPENAI_API_KEY}"
      },
      "metadata": {
        "description": "Semantic code search across entire codebase using Milvus vector database",
        "capabilities": [
          "Index codebase for semantic search",
          "Find relevant code snippets",
          "Search by natural language queries",
          "Cross-file code understanding",
          "40% token reduction vs full codebase loading"
        ],
        "requiresServices": ["milvus"]
      }
    },
    "codeql": {
      "url": "http://127.0.0.1:8000/sse",
      "metadata": {
        "description": "Deep code navigation via call graphs and data flow analysis",
        "capabilities": [
          "Trace call graphs across functions",
          "Data flow analysis",
          "Find all callers/callees",
          "Security vulnerability detection",
          "Custom code queries"
        ],
        "requiresServices": ["codeql-mcp-server"]
      }
    }
  },
  "mcpServerDefaults": {
    "autoStart": true,
    "restartOnFailure": true,
    "timeout": 30000
  },
  "logging": {
    "level": "info",
    "mcpServerLogs": true
  }
}
